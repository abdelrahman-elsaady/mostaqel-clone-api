  "keywords": [
    "freelancing",
    "api",
    "nodejs",
    "express",
    "mongodb",
    "paypal",
    "stripe",
    "jwt",
    "rest-api"
  ],
  "author": "Mostaqel Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mostaqel-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mostaqel-backend/issues"
  },
  "homepage": "https://github.com/your-username/mostaqel-backend#readme",
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/payouts-sdk": "^1.1.1",
    "@paypal/paypal-js": "^8.1.2",
    "ably": "^2.4.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "pusher": "^5.2.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "stripe": "^17.2.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
